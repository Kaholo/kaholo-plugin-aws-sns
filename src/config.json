          {
            "name": "contentBasedDeduplication",
            "viewName": "Enable Content-based Message Deduplication",
            "type": "boolean",
            "description": "For FIFO topics only. If true, enable default message deduplication based on message content. If unchecked, a deduplication ID must be provided for every publish request.",
            "learnUrl": "https://docs.aws.amazon.com/sns/latest/dg/sns-create-topic.html"
          },
          {
            "name": "kmsMasterKeyId",
            "viewName": "Encryption Master KMS Key ID",
            "type": "string",
            "description": "If specified, enable encryption and use the specified key. Otherwise disable encryption.",
            "placeholder": "1234abcd-12ab-34cd-56ef-1234567890ab",
            "learnUrl": "https://docs.aws.amazon.com/sns/latest/dg/sns-create-topic.html"
          },
          {
            "name": "accessPolicyJson",
            "viewName": "Access Policy JSON",
            "type": "text",
            "description": "If specified, set the access policy of the new Topic according to the provided JSON. Can provide the local path of a JSON file on your agent, or pass JS object from code.",
            "placeholder": "/home/user/accessPolicy.json",
            "learnUrl": "https://docs.aws.amazon.com/sns/latest/dg/sns-authentication-and-access-control.html"
          },
          {
            "name": "deliveryRetryPolicyJson",
            "viewName": "Delivery Retry Policy JSON",
            "type": "text",
            "description": "If specified, set the delivery retry policy of the new Topic according to the provided JSON. Can provide the local path of a JSON file on your agent, or pass JS object from code.",
            "placeholder": "/home/user/deliveryRetryPolicy.json",
            "learnUrl": "https://docs.aws.amazon.com/sns/latest/dg/sns-message-delivery-retries.html"
