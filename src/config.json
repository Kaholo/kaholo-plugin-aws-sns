                    "id": "None",
                    "name": "None"
                },
                {
                    "id": "AWS Lambda",
                    "name": "AWS Lambda"
                },
                {
                    "id": "Amazon SQS",
                    "name": "Amazon SQS"
                },
                {
                    "id": "HTTP/S",
                    "name": "HTTP/S"
                },
                {
                    "id": "Platform application endpoint",
                    "name": "Platform application endpoint"
                },
                {
                    "id": "Amazon Kinesis Data Firehose",
                    "name": "Amazon Kinesis Data Firehose"
